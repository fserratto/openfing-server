		"patch-package": "6.2.2",
		"prettier": "2.1.1",
		"ts-node": "9.0.0",
		"typescript": "4.0.2"
	},
	"dependencies": {
		"@babel/runtime": "7.11.2",
		"apollo-errors": "1.9.0",
		"apollo-server-core": "2.17.0",
		"apollo-server-express": "2.17.0",
		"axios": "0.20.0",
		"bcrypt": "5.0.0",
