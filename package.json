{
	"name": "openfing-server",
	"version": "1.1.2",
	"description": "OpenFING Server",
	"scripts": {
		"start:dev": "node -r ./scripts/babel ./src",
		"start:dev:watch": "nodemon",
		"generate-files": "node -r ./scripts/babel ./scripts/generate-files",
		"generate-files:watch": "node -r ./scripts/babel ./scripts/generate-files -w",
		"build": "babel src --out-dir dist --copy-files --extensions '.ts,.js'",
		"deploy": "node -r ./scripts/babel ./scripts/deploy",
		"postinstall": "patch-package",
		"typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sangonz193/openfing-server.git"
	},
	"author": "Santiago Gonzalez",
	"license": "MIT",
	"devDependencies": {
		"@babel/cli": "7.11.6",
		"@babel/core": "7.11.6",
		"@babel/plugin-proposal-class-properties": "7.10.4",
		"@babel/plugin-proposal-decorators": "7.10.5",
		"@babel/plugin-proposal-object-rest-spread": "7.11.0",
		"@babel/plugin-transform-runtime": "7.11.5",
		"@babel/preset-env": "7.11.5",
		"@babel/preset-typescript": "7.10.4",
		"@babel/register": "7.11.5",
		"@graphql-codegen/cli": "1.17.8",
		"@graphql-codegen/typescript": "1.17.9",
		"@graphql-codegen/typescript-resolvers": "1.17.9",
		"@types/bcrypt": "3.0.0",
		"@types/cors": "2.8.7",
		"@types/eslint": "7.2.2",
		"@types/express": "4.17.8",
		"@types/ffprobe": "1.1.0",
		"@types/ffprobe-static": "2.0.0",
		"@types/glob": "7.1.3",
		"@types/jsonwebtoken": "8.5.0",
		"@types/lodash": "4.14.161",
		"@types/moment-timezone": "0.5.30",
		"@types/node": "12.12.55",
		"@types/pg": "7.14.4",
		"@types/pg-copy-streams": "1.2.1",
		"@types/url-join": "4.0.0",
		"@types/uuid": "8.3.0",
		"@types/yup": "0.29.6",
		"@typescript-eslint/eslint-plugin": "4.0.1",
		"@typescript-eslint/parser": "4.0.1",
		"eslint": "7.8.1",
		"eslint-config-prettier": "6.11.0",
